      <Label fx:id="telcc" layoutX="461.0" layoutY="357.0" prefHeight="18.0" prefWidth="266.0" textFill="RED" />
      <TextField fx:id="numcnam" layoutX="460.0" layoutY="375.0" prefHeight="26.0" prefWidth="266.0" promptText="Numero Cnam" />
      <Label fx:id="cnamcc" layoutX="462.0" layoutY="401.0" prefHeight="18.0" prefWidth="266.0" textFill="RED" />
      <Button fx:id="filebtn" layoutX="461.0" layoutY="489.0" mnemonicParsing="false" onAction="#file" prefHeight="26.0" prefWidth="266.0" text="Choisir un fichier" />
      <Label layoutX="462.0" layoutY="463.0" prefHeight="18.0" prefWidth="185.0" text="Optionnelle(pour les médecin)" />
    </children>
</AnchorPane>
