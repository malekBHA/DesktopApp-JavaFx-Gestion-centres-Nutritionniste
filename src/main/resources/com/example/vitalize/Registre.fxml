      <TextField fx:id="addres" layoutX="460.0" layoutY="287.0" prefHeight="26.0" prefWidth="266.0" promptText="Address" />
      <Label fx:id="addresscc" layoutX="461.0" layoutY="313.0" prefHeight="18.0" prefWidth="266.0" textFill="RED" />
